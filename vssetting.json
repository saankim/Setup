{
  "Find replace": true,
  "Open files list": false,
  "Quick open": true,
  "Toggle activity bar": true,
  "Toggle terminal": false,
  "[json]": {
    "editor.defaultFormatter": "HookyQR.beautify"
  },
  "cSpell.allowCompoundWords": true,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.userWords": [
    "skku",
    "badger",
    "rtics",
    "saan",
    "saankim",
    "byt"
  ],
  "codesnap.containerPadding": "2em",
  "codesnap.transparentBackground": true,
  "customizeUI.activityBar": "bottom",
  "customizeUI.activityBarHideSettings": true,
  "customizeUI.fontSizeMap": {
    "13px": "12px",
    "monospace": "12px",
    "tab-title": "12px",
    "window-title": "12px"
  },
  "customizeUI.listRowHeight": 20,
  "customizeUI.stylesheet": {
    ".search-view .search-widgets-container": "padding-top: 0px !important",
    ".suggest-input-container": "padding: 3px 4px 3px !important;"
  },
  "customizeUI.titleBar": "frameless",
  "debug.inlineValues": true,
  "debug.showBreakpointsInOverviewRuler": true,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.acceptSuggestionOnEnter": "smart",
  "editor.accessibilitySupport": "off",
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": true,
  "editor.cursorSurroundingLines": 100,
  "editor.dragAndDrop": false,
  "editor.folding": false,
  "editor.fontFamily": "\"JetBrains Mono\", \"D2Coding ligature\"",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.highlightActiveIndentGuide": false,
  "editor.lineDecorationsWidth": "2ch",
  "editor.lineNumbers": "off",
  "editor.minimap.enabled": false,
  "editor.minimap.maxColumn": 60,
  "editor.minimap.renderCharacters": false,
  "editor.minimap.showSlider": "always",
  "editor.minimap.size": "fill",
  "editor.overviewRulerBorder": false,
  "editor.renderIndentGuides": false,
  "editor.scrollbar.horizontal": "hidden",
  "editor.smoothScrolling": true,
  "editor.suggest.shareSuggestSelections": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wrappingIndent": "none",
  "errorLens.enabledDiagnosticLevels": [
    "error",
    "warning",
    "info"
  ],
  "errorLens.fontStyleItalic": true,
  "errorLens.margin": "2ch",
  "explorer.incrementalNaming": "smart",
  "explorer.sortOrder": "type",
  "files.autoSave": "onFocusChange",
  "files.defaultLanguage": "go",
  "files.exclude": {
    ".gitignore": true,
    ".vscode": true,
    "BUGS.md": true,
    "ChangeLog.md": true,
    "LICENSE": true,
    "go.sum": true
  },
  "git.autoStash": true,
  "git.enableSmartCommit": true,
  "git.postCommitCommand": "sync",
  "go.autocompleteUnimportedPackages": true,
  "go.buildOnSave": "off",
  "go.coverageDecorator": {
    "coveredGutterStyle": "blockblue",
    "coveredHighlightColor": "rgba(64,128,128,0.5)",
    "type": "highlight",
    "uncoveredGutterStyle": "slashyellow",
    "uncoveredHighlightColor": "rgba(128,64,64,0.25)"
  },
  "go.gotoSymbol.includeGoroot": true,
  "go.gotoSymbol.includeImports": true,
  "go.inferGopath": true,
  "go.lintTool": "golangci-lint",
  "go.useCodeSnippetsOnFunctionSuggest": true,
  "go.useLanguageServer": true,
  "goOutliner.extendExplorerTab": true,
  "keyboard.touchbar.enabled": false,
  "lowlightgoerrors.MediumOpacity": "0.3",
  "path-intellisense.autoSlashAfterDirectory": true,
  "scrolloff.alwaysCenter": true,
  "search.exclude": {
    "**/data/**": true
  },
  "search.smartCase": true,
  "search.useGlobalIgnoreFiles": true,
  "sidebar.scrollbar.horizontal": "hidden",
  "sidebar.scrollbar.vertical": "hidden",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.cursorWidth": 2,
  "terminal.integrated.rightClickBehavior": "default",
  "terminal.integrated.scrollback": 100000,
  "update.mode": "manual",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "window.restoreFullscreen": true,
  "window.title": "${rootName}",
  "window.titleBarStyle": "native",
  "workbench.activityBar.visible": true,
  "workbench.colorCustomizations": {
    "editorGroup.border": "#00000000",
    "scrollbar.shadow": "#00000000",
    "editor.selectionBackground": "#111111",
    "editor.selectionHighlightBackground": "#000000",
    "editor.inactiveSelectionBackground": "#111111"
  },
  "workbench.colorTheme": "Atom One Real Dark",
  "workbench.editor.showTabs": false,
  "workbench.editor.tabCloseButton": "left",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.statusBar.visible": false,
  "workbench.tree.renderIndentGuides": "none",
  "git.confirmSync": false,
  "editor.find.autoFindInSelection": "multiline"
}