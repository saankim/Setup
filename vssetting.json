{
    "Find replace": true,
    "Open files list": false,
    "Quick open": true,
    "Toggle activity bar": true,
    "Toggle terminal": false,
    "[json]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
    "[markdown]": {
        "editor.wordWrap": "wordWrapColumn",
        "editor.wordWrapColumn": 80
    },
    "al.backgroundCodeAnalysis": false,
    "breadcrumbs.enabled": false,
    "browser-preview.format": "png",
    "cSpell.allowCompoundWords": true,
    "cSpell.diagnosticLevel": "Hint",
    "cSpell.spellCheckDelayMs": 800,
    "cSpell.userWords": [
        "skku",
        "badger",
        "rtics",
        "saan",
        "saankim",
        "byt"
    ],
    "codesnap.containerPadding": "2em",
    "codesnap.transparentBackground": true,
    "codestream.email": "kssan99@gmail.com",
    "customizeUI.activityBar": "regular",
    "customizeUI.activityBarHideSettings": true,
    "customizeUI.stylesheet": {
        ".search-view .search-widgets-container": "padding-top: 0px !important",
        ".suggest-input-container": "padding: 3px 4px 3px !important;"
    },
    "customizeUI.titleBar": "inline",
    "debug.inlineValues": true,
    "debug.showBreakpointsInOverviewRuler": true,
    "diffEditor.ignoreTrimWhitespace": false,
    "editor.acceptSuggestionOnEnter": "smart",
    "editor.accessibilitySupport": "off",
    "editor.cursorBlinking": "phase",
    "editor.cursorSmoothCaretAnimation": true,
    "editor.cursorSurroundingLines": 20,
    "editor.dragAndDrop": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.folding": false,
    "editor.fontFamily": "\"Fira Code\", \"Jetbrains Mono\", \"D2Coding ligature\"",
    "editor.fontLigatures": true,
    "editor.fontSize": 13,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.highlightActiveIndentGuide": false,
    "editor.lineDecorationsWidth": "2ch",
    "editor.lineNumbers": "off",
    "editor.minimap.enabled": false,
    "editor.minimap.maxColumn": 60,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fill",
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestionsDelay": 100,
    "editor.renderIndentGuides": false,
    "editor.renderWhitespace": "none",
    "editor.rulers": [
        80
    ],
    "editor.scrollbar.horizontal": "hidden",
    "editor.semanticTokenColorCustomizations": {
        "enabled": true,
        "rules": {
            "*.declaration": {
                "bold": true,
                "underline": true
            },
            "comment": "#ff0000",
            "property.readonly": "#35166d"
        }
    },
    "editor.smoothScrolling": true,
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.statusBar.visible": true,
    "editor.suggestSelection": "first",
    "editor.tabSize": 2,
    "editor.wordWrap": "wordWrapColumn",
    "editor.wrappingIndent": "none",
    "errorLens.delay": 500,
    "errorLens.enabledDiagnosticLevels": [
        "error",
        "warning",
        "info"
    ],
    "errorLens.fontStyleItalic": true,
    "errorLens.margin": "2ch",
    "explorer.decorations.colors": false,
    "explorer.incrementalNaming": "smart",
    "explorer.sortOrder": "type",
    "extensions.ignoreRecommendations": false,
    "files.autoSave": "onFocusChange",
    "files.defaultLanguage": "go",
    "files.exclude": {
        ".vscode": true,
        "BUGS.md": true,
        "ChangeLog.md": true,
        "LICENSE": true,
        "go.sum": true
    },
    "git.autoStash": true,
    "git.confirmSync": false,
    "git.enableSmartCommit": true,
    "git.postCommitCommand": "sync",
    "go.autocompleteUnimportedPackages": true,
    "go.buildOnSave": "off",
    "go.coverageDecorator": {
        "coveredGutterStyle": "blockblue",
        "coveredHighlightColor": "rgba(64,128,128,0.5)",
        "type": "highlight",
        "uncoveredGutterStyle": "slashyellow",
        "uncoveredHighlightColor": "rgba(128,64,64,0.25)"
    },
    "go.lintFlags": [
        "--fast",
        "--concurrency=2",
        "--max-same-issues=1",
        "--max-issues-per-linter=3"
    ],
    "go.lintOnSave": "file",
    "go.lintTool": "golangci-lint",
    "go.liveErrors": {
        "delay": 600,
        "enabled": true
    },
    "go.useCodeSnippetsOnFunctionSuggest": true,
    "go.useLanguageServer": true,
    "goOutliner.extendExplorerTab": true,
    "kite.showGoBetaNotification": false,
    "kite.showWelcomeNotificationOnStartup": false,
    "latex-workshop.view.pdf.viewer": "tab",
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.wait": 300,
    "lowlightgoerrors.MediumOpacity": "0.3",
    "markdown.preview.fontFamily": "\"HCR Dotum\",-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    "nasc-touchbar.addCursorBelow": false,
    "nasc-touchbar.docs": true,
    "nasc-touchbar.enableEditorGroup": true,
    "nasc-touchbar.jumpToBracket": true,
    "nasc-touchbar.peekDefinition": true,
    "nasc-touchbar.showCommands": false,
    "nasc-touchbar.showReferences": true,
    "nasc-touchbar.togglePanel": false,
    "path-intellisense.autoSlashAfterDirectory": true,
    "python.jediEnabled": false,
    "python.languageServer": "Microsoft",
    "resmon.show.mem": false,
    "scrolloff.alwaysCenter": true,
    "search.exclude": {
        "**/data/**": true
    },
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "sidebar.scrollbar.horizontal": "hidden",
    "sidebar.scrollbar.vertical": "hidden",
    "spotify.showLyricsButton": false,
    "spotify.showMuteUnmuteVolumeButton": false,
    "spotify.showPlayPauseButton": false,
    "spotify.showPreviousButton": false,
    "spotify.showSignInButton": false,
    "spotify.showSignOutButton": false,
    "spotify.trackInfoClickBehaviour": "play_pause",
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "line",
    "terminal.integrated.cursorWidth": 2,
    "terminal.integrated.rightClickBehavior": "default",
    "todo-tree.general.statusBar": "total",
    "update.mode": "manual",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "window.restoreFullscreen": true,
    "window.title": "${rootName}",
    "window.titleBarStyle": "native",
    "window.zoomLevel": 0,
    "workbench.activityBar.visible": true,
    "workbench.colorCustomizations": {
        "editor.inactiveSelectionBackground": "#111111",
        "editor.selectionBackground": "#050505",
        "editor.selectionHighlightBackground": "#000000",
        "editorGroup.border": "#00000000",
        "editorRuler.foreground": "#242424"
    },
    "workbench.colorTheme": "Atom One Real Dark",
    "workbench.editor.scrollToSwitchTabs": true,
    "workbench.editor.showTabs": true,
    "workbench.editor.tabCloseButton": "off",
    "workbench.iconTheme": "simple-icons",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.statusBar.visible": false,
    "workbench.tree.renderIndentGuides": "none",
    "zenMode.fullScreen": false
}